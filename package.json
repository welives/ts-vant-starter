    "eslint": "^9.2.0",
    "eslint-plugin-format": "^0.1.1",
    "eslint-plugin-vue": "^9.26.0",
    "mockjs": "^1.1.0",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "postcss-px-to-viewport-8-plugin": "^1.2.5",
    "prettier": "^3.2.5",
    "sass": "^1.77.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.2.11",
    "vite-plugin-mock": "^3.0.2",
    "vue-tsc": "^2.0.18"
