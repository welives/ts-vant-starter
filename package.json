{
  "name": "vite-starter",
  "type": "module",
  "version": "1.0.0",
  "description": "开箱即用的基于 Vite + Pinia + Vant + TailwindCSS + TypeScript 的模板",
  "author": "welives",
  "license": "MIT",
  "keywords": [
    "Vite",
    "Vue3",
    "Pinia",
    "Vant",
    "TailwindCSS",
    "TypeScript",
    "ESLint",
    "Prettier"
  ],
  "scripts": {
    "dev": "vite",
