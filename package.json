    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.0",
    "@rushstack/eslint-patch": "^1.10.2",
    "@tsconfig/node18": "^18.2.4",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.12.12",
    "@vant/auto-import-resolver": "^1.1.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
